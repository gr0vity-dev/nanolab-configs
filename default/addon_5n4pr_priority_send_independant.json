{
    "testcase": "5n4pr_send_independant_cpu_limit",
    "description": "Publish 3* 50 individual send+receive blocks from multiple accounts and await their confirmation",
    "docker_tags": [
        "nanocurrency/nano:V25.0RC3"
    ],
    "commands": [
	{
            "description": "Publish 3 * 50 independant send blocks",
            "type": "threaded",
            "commands": [
                {
                    "type": "python",
                    "class": "NodeInteraction",
                    "group": "g1",
                    "method": "publish_create",
                    "delay": 0,
                    "variables": {
                        "params": {
                            "type": "send_wait_conf",
                            "source_seed": "10CA10000000000000000000000000000000000000000000000000000000000A",
                            "destination_seed": "10CA100000000000000000000000000000000000000000000000000000000999",
			                "send_amount_raw" : 958838493693761105859604119549,
                            "source_start_index": 30,
                            "block_count": 10,
                            "is_independent": true,
                            "timeout_s": 10,
                            "node_name": "nl_pr1"
                        }
                    }
                }
            ]
        }
    ]
}
